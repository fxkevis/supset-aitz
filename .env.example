# AI Browser Agent Environment Configuration
# Copy this file to .env and fill in your actual values

# AI Model API Keys (at least one is required)
CLAUDE_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# AI Model Configuration
AI_PRIMARY_MODEL=claude
AI_FALLBACK_MODEL=openai
AI_MAX_TOKENS=4000
AI_TEMPERATURE=0.7

# Browser Configuration
BROWSER_HEADLESS=false
BROWSER_TYPE=chrome
BROWSER_TIMEOUT=30
BROWSER_PROFILE_PATH=

# Security Configuration
SECURITY_REQUIRE_PAYMENT_CONFIRMATION=true
SECURITY_REQUIRE_DELETION_CONFIRMATION=true
SECURITY_MAX_TASK_DURATION=3600

# Application Configuration
DEBUG_MODE=false
LOG_FILE=logs/app.log
DATA_DIRECTORY=data